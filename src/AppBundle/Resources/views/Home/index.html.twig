{% block javascripts %}
    {{ parent() }}
    <script type="text/javascript">
        var pictureNo = 1;

        $(document).ready(function() {

            setTimeout(changeParallaxBackground, 5000);

        });

        function changeParallaxBackground()
        {
            pictureNo++;
            if(pictureNo > 3){
                pictureNo = 1;
            }

            $("#parallax-main").css('background-image', 'url({{ asset('images/parallax/') }}' + pictureNo + '.jpg)');
            setTimeout(changeParallaxBackground, 9000);
        }

    </script>
