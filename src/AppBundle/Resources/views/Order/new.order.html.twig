{% extends "::base.html.twig" %}

{% block title %}
    Nauja paslaptis - {{ parent() }}
{% endblock %}

{% block stylesheets %}
    {{ parent() }}
{% endblock %}

{% block body %}
    <div class="body-order">
        <div class="row">
            <div class="col-lg-12 col-lg-offset-3">
                <h2>{{ 'order.secretbox' | trans }}</h2>
            </div>
        </div>
        <div class="row">
            <div class="col-lg-6 col-lg-offset-3">



            <div class="panel panel-default">
                    <div class="panel-heading">
                        {{ 'user.name' | trans }}, {{ 'user.surname' | trans }}
                    </div>
                    <div class="panel-body">{{ app.user.firstName }} {{ app.user.lastName }}</div>
                </div>
                {{ form(form) }}
            </div>
        </div>
    </div>


{% endblock %}


{% block javascripts %}
    {{ parent() }}
{% endblock %}